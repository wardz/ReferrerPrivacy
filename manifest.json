{
  "manifest_version": 3,
  "name": "ReferrerPrivacy",
  "description": "Strips the referrer header from cross-origin GET requests & outbound link clicks.",
  "version": "1.0.0",
  "minimum_chrome_version": "126",
  "icons": {
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "declarativeNetRequest",
    "userScripts",
    "storage"
  ]
}
