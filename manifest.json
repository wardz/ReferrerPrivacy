{
  "manifest_version": 3,
  "name": "ReferrerPrivacy",
  "description": "Blocks referrer header for cross-origin GET requests.",
  "version": "1.0.0",
  "icons": {
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "declarativeNetRequest",
    "scripting"
  ]
}
